     "input": [
      "# compute the Jaccard distance between two sentences\n",
      "def DistJaccard(str1, str2):\n",
      "    if str1 != '' and str2 != '':\n",
      "        str1 = set(str1.split())\n",
      "        str2 = set(str2.split())\n",
      "        return 1.0 - float(len(str1 & str2)) / len(str1 | str2)\n",
      "    else:\n",
      "        return numpy.nan"
     ],
